{
  "name": "smart",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write app",
    "format:check": "prettier --check app",
    "test": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@tamagui/font-inter": "^1.33.2",
    "@tamagui/lucide-icons": "^1.33.2",
    "@tamagui/react-native-media-driver": "^1.33.2",
    "@tamagui/shorthands": "^1.33.2",
    "@tamagui/themes": "^1.33.2",
    "@tanstack/react-query": "^4.29.13",
    "expo": "^49.0.3",
    "expo-constants": "~14.4.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "^5.0.2",
    "expo-router": "^2.0.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "tamagui": "^1.33.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tamagui/babel-plugin": "^1.33.2",
    "@types/react": "~18.2.14",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^8.42.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "metro": "0.76.5",
    "metro-resolver": "0.76.5"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "private": true
